<div class="linkContainer">

    <br>
    <br>
    <h1 class="eventHead">Your saved events:</h1>

    <div class="col-md-12 eventOptions">
        <div class="col-md-12 savedList">
            <ul style="list-style-type:none">
                {{#each events}}
                <li>
                    <div class="card text-white text-center bg-success mb-3" style="max-width: 18rem;">
                        <div class="card-header">{{ this.eventName }} </div>
                        <div class="card-body">
                            <h5 class="card-title"> {{ this.eventCity }} </h5>
                            <button data-id="{{this.id}}" class="btn btn-danger chosen" href="{{ this.eventURL }}" target="_blank">Click for tickets.</button>
                        </div>
                    </div>
                </li>
                {{/each}}
            </ul>
        </div>
    </div>

</div>